<!-- Start Work Process -->
<section class="process">
  <div class="container p-tb-10">
    <div class="row st-htext">
      <h2>Work Process</h2>
    </div>
    <div class="row steps">
      <div class="col-lg-4 col-sm-12 step">
        <h2>01</h2>
        <h4 data-aos="fade-up" data-aos-duration="1000">Planning & Site Preparation</h4>
        <p>Evaluating the site, obtaining permits, and preparing the ground for construction.</p>
      </div>
      <div class="col-lg-4 col-sm-12 step step-mid">
        <h2>02</h2>
        <h4 data-aos="fade-up" data-aos-duration="1200">Foundation & Structural Work</h4>
        <p>Building the foundation, erecting structural elements, and ensuring stability.</p>
      </div>
      <div class="col-lg-4 col-sm-12 step">
        <h2>03</h2>
        <h4 data-aos="fade-up" data-aos-duration="1400">Finishing & Final Touches</h4>
        <p>Installing interiors, electrical, plumbing, and completing the final inspections.</p>
      </div>
    </div>
  </div>
</section>
<!-- End Work Process -->

